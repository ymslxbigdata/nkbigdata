<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering SYSTEM "hibernate-tools/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <type-mapping>
     <sql-type jdbc-type="VARCHAR" length="1" hibernate-type="boolean"/>
     <sql-type jdbc-type="CHAR" length="1" hibernate-type="boolean"/>
     <sql-type jdbc-type="TIMESTAMP" hibernate-type="java.time.LocalDateTime"/>
     <sql-type jdbc-type="DATE" hibernate-type="java.time.LocalDate"/>
  </type-mapping>
</hibernate-reverse-engineering>
